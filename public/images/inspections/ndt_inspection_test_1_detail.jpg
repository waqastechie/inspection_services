<!-- SVG placeholder for detailed inspection image -->
<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <rect width="600" height="400" fill="#f8f9fa"/>
  <rect x="20" y="20" width="560" height="360" fill="#ffffff" stroke="#333" stroke-width="1"/>
  
  <!-- Close-up weld detail -->
  <rect x="100" y="100" width="400" height="200" fill="#e2e8f0" stroke="#4a5568" stroke-width="2"/>
  
  <!-- Weld lines -->
  <path d="M 150 180 Q 200 160 250 180 Q 300 200 350 180 Q 400 160 450 180" 
        stroke="#2d3748" stroke-width="4" fill="none"/>
  
  <!-- Defect indication -->
  <circle cx="320" cy="185" r="8" fill="#f56565" opacity="0.7"/>
  <circle cx="340" cy="175" r="6" fill="#f56565" opacity="0.7"/>
  
  <!-- Measurement lines -->
  <line x1="300" y1="150" x2="360" y2="150" stroke="#e53e3e" stroke-width="2"/>
  <text x="330" y="145" text-anchor="middle" font-family="Arial" font-size="10" fill="#e53e3e">
    25mm
  </text>
  
  <text x="300" y="50" text-anchor="middle" font-family="Arial" font-size="14" fill="#333">
    Weld Joint Detail - Main Boom Section
  </text>
  <text x="300" y="70" text-anchor="middle" font-family="Arial" font-size="10" fill="#666">
    Linear indications detected in HAZ
  </text>
  
  <!-- Severity indicators -->
  <text x="50" y="350" font-family="Arial" font-size="10" fill="#333">
    Defect Severity: Category 3 (Monitor)
  </text>
  <text x="50" y="365" font-family="Arial" font-size="10" fill="#333">
    Action Required: Monitor at next inspection
  </text>
</svg>
